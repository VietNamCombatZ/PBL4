# filled by user; see .env.example
# General
EPOCH_SEC=10
OFFLINE=false
CACHE_TTL_SEC=86400
HTTP_TIMEOUT_SEC=10
HTTP_RETRIES=3
BACKOFF_FACTOR=0.6

# Sources
ENABLE_SEA=false
ENABLE_GROUND=true
ENABLE_SAT=true
ENABLE_AIR=true

# Clustering and BBox
ENABLE_CLUSTERING=true
CLUSTER_RADIUS_KM=20
BBOX_MIN_LAT=-90
BBOX_MAX_LAT=90
BBOX_MIN_LON=-180
BBOX_MAX_LON=180

# Link building
ELEVATION_MIN_DEG=10
MAX_RANGE_AIR_AIR=400
MAX_RANGE_GROUND_GROUND=80
MAX_RANGE_GROUND_AIR=300
MAX_RANGE_GROUND_SAT=2000
MAX_RANGE_AIR_SAT=2500
MAX_RANGE_SAT_SAT=5000

# Link model defaults
FREQ_HZ=2.4e9
BW_HZ=20e6
P_TX_DBM=20
NOISE_DBM=-100
PROC_QUEUE_MS=2

# ACO defaults
ANTS=30
ITERS=60
ALPHA=1.0
BETA=3.0
RHO=0.2
XI=0.1
Q0=0.2
TAU0=0.2
MMAS=true
TAU_MIN=0.01
TAU_MAX=2.0
WEIGHTS=0.5,0.2,0.2,0.1

# Docker nodes
NODES=50
